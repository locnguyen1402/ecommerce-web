  },
  "devDependencies": {
    "@bufbuild/buf": "^1.29.0",
    "@bufbuild/protoc-gen-es": "^1.7.0",
    "@types/qs": "6.9.7",
    "@vklink/eslint-config": "workspace:*",
    "@vklink/typescript-config": "workspace:*",
    "eslint": "^8.56.0",
